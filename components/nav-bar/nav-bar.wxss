.img-view image{
  vertical-align:middle;
  width: 50rpx;
}
.nav-bar{
  display: flex;
  background: #fff;
  align-items: center;
  justify-content: space-around;
  font-size: 30rpx;
  color: #999999;
  box-sizing: border-box;
  padding-left: 40rpx;
  border-top: 1px  solid #eeeeee;
  border-bottom: 1px  solid #eeeeee;
}
.nav-bar>view{
   height: 96rpx;
   line-height: 96rpx;
   box-sizing: border-box;
   padding: 0 40rpx;
}
.active{
  color: #e60012;
}
.up-down{
  width: 20rpx;
}